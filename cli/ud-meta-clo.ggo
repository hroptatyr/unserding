args "--unamed-opts -a meta_args_info -f meta_parser"
package "ud-meta"
usage "ud-meta [OPTION]... SYMIDX..."
description "\
Query symbol or index SYMIDX."

section "Network options"
option "port" p
	"Multicast control channel port"
	int default="8653" optional

option "beef" - 
	"Multicast payload channel  (default determined by network)"
	int optional

