args "--unamed-opts"
usage " [server-options] module [...]"
##description "unserding daemon"

section "Server options"
option "config" c
	"Configuration file"
	string optional typestr="FILE"
option "daemon" d
	"Detach from tty and run as daemon"
	flag off
option "pidfile" p
	"Output pid of server process into this file"
	string optional typestr="FILE"
option "workers" w
	"Number of concurrent worker threads (default: # of cores)"
	int optional
